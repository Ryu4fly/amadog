<h1>Now Available to Walk Your Dog</h1>
<ul class="list-group">
  <% @users.each do |user| %>
    <li class="list-group-item"><%= link_to user.username, user_path(user) %> <%= user.address %></li>
  <% end %>
</ul>
<br /><div id="map"
     style="width: 100%; height: 600px;"
     data-markers="<%= @markers.to_json %>"
     data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"></div>